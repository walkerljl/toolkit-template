<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-autowire="byName">

    <import resource="spring-interceptor-log-integration.xml"/>
    <import resource="spring-interceptor-log-service.xml"/>

    <bean id="helloFacade" class="org.walkerljl.toolkit.template.service.facade.impl.HelloFacadeImpl"></bean>
    <bean id="helloFacadeClient" class="org.walkerljl.toolkit.template.service.integration.impl.HelloFacadeClientImpl">
        <property name="helloFacade" ref="helloFacade"></property>
    </bean>

</beans>
